# Telegram Bot Token
# Obtener en: https://t.me/BotFather
TELEGRAM_BOT_TOKEN=your_token_here

# Google Gemini API Key (Principal LLM)
# Obtener en: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_key_here
# Modelo (opcional): gemini-1.5-flash, gemini-1.5-pro, gemini-2.0-flash-exp
GEMINI_MODEL=gemini-1.5-flash

# OpenRouter API Key (Opcional - Fallback)
# Obtener en: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Serper API Key (para b√∫squedas con Google)
# Obtener en: https://serper.dev/
SERPER_API_KEY=your_serper_key_here

# Railway Configuration
RAILWAY_STATIC_URL=

# Environment
NODE_ENV=production

# Logging
LOG_LEVEL=info

# MCP Configuration
MCP_PLAYWRIGHT_BROWSER=chromium
MCP_PLAYWRIGHT_HEADLESS=true

# OpenClaw
OPENCLAW_ENABLED=true
OPENCLAW_CONFIG_PATH=openclaw.json
OPENCLAW_THINKING=minimal
OPENCLAW_TIMEOUT_SECONDS=120
